{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "none",
    "svelteIndentScriptAndStyle": true,
    "printWidth": 100,
    "tabWidth": 4,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
