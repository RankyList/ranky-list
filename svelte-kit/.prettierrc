{
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "tabWidth": 4
      }
    },
    {
      "files": "*.html",
      "options": {
        "tabWidth": 4
      }
    }
  ],
  "pluginSearchDirs": ["."],
  "printWidth": 150,
  "quoteProps": "consistent",
  "singleQuote": true,
  "svelteIndentScriptAndStyle": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
