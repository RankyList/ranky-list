{
  "name": "svelte-kit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED='0' vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:unit:ui": "vitest -w --ui --no-open",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write . && eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "fixtures": "tsx fixtures/index.ts"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "follow-redirects": "^1.15.2",
    "pocketbase": "^0.14.0",
    "sveltekit-superforms": "^0.8.3",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@floating-ui/dom": "^1.2.7",
    "@playwright/test": "1.33.0",
    "@poppanator/sveltekit-svg": "^3.0.0",
    "@skeletonlabs/skeleton": "^1.2.5",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/svelte": "^7.0.7",
    "@storybook/sveltekit": "^7.0.7",
    "@storybook/testing-library": "^0.1.0",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/kit": "1.15.9",
    "@tabler/icons-svelte": "^2.17.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/follow-redirects": "^1.14.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
    "eslint-plugin-jsdoc": "^43.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte": "^2.27.0",
    "pocketbase-typegen": "^1.1.8",
    "postcss": "^8.4.23",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.21.0",
    "sass": "^1.62.1",
    "storybook": "^7.0.7",
    "svelte": "^3.58.0",
    "svelte-awesome-color-picker": "^2.2.2",
    "svelte-check": "^3.2.0",
    "svelte-dnd-action": "^0.9.22",
    "svelte-legos": "^0.2.1",
    "svelte-meta-tags": "^2.7.0",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.3.1",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  }
}
