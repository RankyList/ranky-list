{
  "name": "svelte-kit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED='0' vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "test:unit:ui": "vitest -w --ui --no-open",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write . && eslint . --fix",
    "story:dev": "histoire dev --host",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "fixtures": "tsx fixtures/index.ts"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "follow-redirects": "^1.15.2",
    "pocketbase": "^0.16.0",
    "sveltekit-superforms": "^1.5.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@floating-ui/dom": "^1.5.1",
    "@histoire/plugin-svelte": "^0.16.4",
    "@playwright/test": "1.37.0",
    "@poppanator/sveltekit-svg": "^4.0.1",
    "@skeletonlabs/skeleton": "^1.12.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "1.22.6",
    "@tabler/icons-svelte": "^2.30.0",
    "@tailwindcss/forms": "^0.5.4",
    "@types/follow-redirects": "^1.14.1",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "autoprefixer": "^10.4.15",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "histoire": "^0.16.4",
    "pocketbase-typegen": "^1.1.11",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "rollup": "^3.28.0",
    "sass": "^1.65.1",
    "svelte": "^4.2.0",
    "svelte-awesome-color-picker": "^2.4.7",
    "svelte-check": "^3.5.0",
    "svelte-dnd-action": "^0.9.24",
    "svelte-meta-tags": "^2.8.0",
    "svelte-preprocess": "^5.0.4",
    "svgo": "^3.0.2",
